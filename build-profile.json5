          ]
        }
      ]
    },
    {
      "name": "home",
      "srcPath": "./features/home"
    },
    {
      "name": "scheme",
      "srcPath": "./features/scheme"
    },
    {
      "name": "mine",
      "srcPath": "./features/mine",
    },
    {
      "name": "login",
      "srcPath": "./features/login",
    },
    {
      "name": "question",
      "srcPath": "./features/question"
    },
    {
      "name": "network",
      "srcPath": "./commons/network",
    },
    {
      "name": "uicomponents",
      "srcPath": "./commons/uicomponents",
    },
    {
      "name": "utils",
      "srcPath": "./commons/utils",
    }
  ]
